{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "prettier": true,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "interface-name": false,
    "curly": false
  }
}
